    {
        "id": "9ce74cc0bc7abe45",
        "type": "function",
        "z": "823e8b7aabfc2916",
        "name": "split msg",
        "func": "// split messages by length and first Char\n// can be reduced to\n//const firstChar = msg.payload[0];\n//const secondChar = msg.payload[1];\n//msg.payload = secondChar;\n//if (firstChar === \"1\") {\n //   return [null, msg];\n//} else if (firstChar === \"2\") {\n//    return [msg, null];\n//} \n\nconst firstChar = msg.payload[0];\nconst secondChar = msg.payload[1];\n\n//msg.payload = secondChar;\n\nif (msg.payload.length == 4){\n    if (firstChar === \"1\") {\n        msg.length = msg.payload.length;\n        msg.payload = secondChar;\n        return [msg, null, null];\n    } else if (firstChar === \"2\") {\n        msg.length = msg.payload.length;\n        msg.payload = secondChar;\n        return [null, msg, null];\n    }\n}\n\nmsg.payload = msg.payload;\nmsg.length = msg.payload.length;\nreturn [null, null, msg];",
